    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.15.0",
    "babel-loader": "^8.2.2",
    "hugo-bin-extended": "^0.80.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
